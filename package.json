{
  "name": "iso-15924",
  "version": "0.0.0",
  "description": "ISO 15924 list of scripts",
  "license": "MIT",
  "keywords": [
    "iso-15924",
    "iso",
    "15924",
    "list",
    "script",
    "codes",
    "names",
    "information"
  ],
  "dependencies": {
    "datamap-interface": "^0.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wooorm/iso-15924.git"
  },
  "files": [
    "index.js",
    "data/iso-15924.json"
  ],
  "author": "Titus Wormer <tituswormer@gmail.com>",
  "devDependencies": {
    "browserify": "^11.0.1",
    "chalk": "^1.0.0",
    "eslint": "^1.0.0",
    "esmangle": "^1.0.1",
    "istanbul": "^0.3.0",
    "jscs": "^2.0.0",
    "jscs-jsdoc": "^1.0.0",
    "markdown-table": "^0.4.0",
    "mdast": "^0.29.0",
    "mdast-comment-config": "^0.1.2",
    "mdast-github": "^0.3.2",
    "mdast-lint": "^0.4.2",
    "mdast-slug": "^0.1.1",
    "mdast-validate-links": "^0.3.1",
    "mocha": "^2.0.0",
    "plain-text-data-to-json": "^0.1.1"
  },
  "scripts": {
    "test-api": "mocha --check-leaks test.js",
    "test-coverage": "istanbul cover _mocha -- test.js",
    "test-travis": "npm run test-coverage",
    "test": "npm run test-api",
    "lint-api": "eslint .",
    "lint-style": "jscs --reporter inline .",
    "lint": "npm run lint-api && npm run lint-style",
    "make": "npm run lint && npm run test-coverage",
    "crawl": "./crawl.sh",
    "postcrawl": "node script/build-data.js && node script/build-support.js",
    "bundle": "browserify index.js --no-builtins -s iso15924 > iso-15924.js",
    "postbundle": "esmangle iso-15924.js > iso-15924.min.js",
    "build-md": "mdast . --quiet",
    "build": "npm run crawl && npm run bundle && npm run build-md"
  }
}
